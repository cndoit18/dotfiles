{
  "default_model": "aihubmix",
  "models": {
    "aihubmix": {
      "provider": "openai",
      "model": "{{ .ai.model | trim }}",
      "max_context_size": 128000
    }
  },
  "providers": {
    "openai": {
      "type": "openai_legacy",
      "base_url": "{{ .ai.endpoint | trimSuffix "/" }}",
      "api_key": "{{ .ai.token }}"
    }
  },
  "loop_control": {
    "max_steps_per_run": 100,
    "max_retries_per_step": 5
  },
  "services": {}
}
